<div class="modal-content">
    <div class="modal-header">
        <h5 class="modal-title">export to protractor</h5>
    </div>
    <div class="modal-body">
        <div class="card-panel export-div">
            <pre>
                <%= @spec_data %>
            </pre>
        </div>
    </div>
</div>
<div class="modal-footer">
    <a class="btn" id="copyBtn" data-clipboard-target=".export-div">copy to clipboard</a>
    <a href="#!" class="modal-close btn-flat modal-action">done</a>
</div>