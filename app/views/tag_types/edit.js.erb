$("#spec-modal").closeModal();
$("#small-modal").html("<%= j(render 'edit') %>")
$('select').material_select();
$(".edit_tag_type").validate();

<% if @tag_type.tag_type_group_id %>
    $('#colorDisplay').css('background-color', '<%= @tag_type.tag_type_group.color %>');
    $('#colorDisplay').show();
    $('#colorPickerDiv').hide();
<% end %>

<% unless @tag_type.errors.any? %>
    $("#small-modal").openModal();
<% end %>